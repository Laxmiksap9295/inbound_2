<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog   class="Dialog sapUiSizeCompact" >
		<customHeader>
			<Bar class="Inbound_Bar">
				<contentMiddle>
					<Label text="{i18n>Title20}" class="Inbound_Title" design="Bold"></Label>
				</contentMiddle>
				<contentRight>
					<Button icon="sap-icon://decline" press="onClickBack" tooltip="{i18n>Tooltip2}" class="Inbound_NavBtn"></Button>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<f:SimpleForm layout="ResponsiveGridLayout" 
							labelSpanXL="5" labelSpanL="4" labelSpanM="4" labelSpanS="4" 
							adjustLabelSpan="false" emptySpanXL="2" emptySpanL="4" emptySpanM="4" emptySpanS="0" 
							columnsXL="1" columnsL="1" columnsM="1"  singleContainerFullSize="false" class="Inbound_SimpleForm">
				<f:content>
					<Label text="{i18n>Label3}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{Approve>/Wbid}" enabled="false" class="Inbound_InputBrd" ></Input>
					<Label text="{i18n>Label6}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{Approve>/Vehno}" enabled="false" class="Inbound_InputBrd"></Input>
					<Label text="{i18n>Label22}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{path:'Approve>/Erdat',formatter:'ZGT_MM_INBOUND.Util.Formatter.Date'}" 
						   enabled="false"	class="Inbound_InputBrd" ></Input>
					<Label text="{i18n>Column3}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{path:'Approve>/Ertim/ms',formatter:'ZGT_MM_INBOUND.Util.Formatter.Time'}" enabled="false" class="Inbound_InputBrd"></Input>
					<Label text="{i18n>Label23}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{Approve>/Remark}" enabled="false" 
					class="Inbound_InputBrd"></Input>
					<Label text="{i18n>Label10}" design="Bold" class="sapUiTinyMarginTop"></Label>
					<Input value="{Approve>/Reason}" maxLength="40"
					class="Inbound_InputBrd"></Input>
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="{i18n>Text17}" press="fnApproveReject"  id="id_BtnApprove" class="Inbound_BtnClr"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>Text18}" press="fnApproveReject" id="id_BtnReject" class="Inbound_BtnClr"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>