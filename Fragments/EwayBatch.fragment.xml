<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog noDataText="{i18n>NoBatchFound}" contentWidth="70%" contentHeight="70%" class="newTableClass" title="{i18n>SelectBatch}">
		<SearchField placeholder="{i18n>Search}" class=" sapUiSizeCompact sapUiSmallMarginBeginEnd sapUiTinyMarginTop" id="id_SearchFieldpo" visible="true" change="fnBatchSearch"
			liveChange="fnBatchSearch" width="97%" />
		<Table noDataText="{i18n>NoBatchFound}" id="id_BatchSelectTab" inset="false" type="Active" items="{JMEwayList>/}"
			itemPress="_handleConfirmBatch" selectionChange="_handleConfirmRef" class="cl_tableBatch">
			<!--itemPress="fnBillRefClose" selectionChange="fnBillRefClose">-->
			<columns>
				<Column width="auto" hAlign="Center">
					<header>
						<Text text="{i18n>Batch}"/>
					</header>
				</Column>
				<Column minScreenWidth="Tablet" width="auto" demandPopin="true" hAlign="Center">
					<header>
						<Text text="{i18n>Text21}"/>
					</header>
				</Column>
				<Column minScreenWidth="Tablet" width="auto" hAlign="Center">
					<header>
						<Text text="{i18n>Ebeln}"/>
					</header>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="auto">
					<header>
						<Text text="{i18n>Ebelp}"/>
					</header>
				</Column>
			</columns>
			<items>
				<ColumnListItem id="id_BillTable" itemPress="_handleConfirmRef" selectionChange="_handleConfirmRef" type="Active">
					<cells>
						<Text text="{JMEwayList>Matnr}"/>
						<Text text="{JMEwayList>Matnr}"/>
						<Text text="{JMEwayList>Ebeln}"/>
						<Text text="{JMEwayList>Ebelp}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		<beginButton>
			<Button text="{i18n>Label17}" press="fnCloseBatch"/>	
		</beginButton>
	</Dialog>
</core:FragmentDefinition>