<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog class="Dialog sapUiSizeCompact" contentWidth="100%" stretchOnPhone="true" resizable="true" verticalScrolling="true">
		<customHeader>
			<Bar class="Inbound_Bar">
				<contentMiddle>
					<Label text="{i18n>Title12}" class="Inbound_Title" design="Bold"></Label>
				</contentMiddle>
				<contentRight>
					<Button icon="sap-icon://decline" press="onClosePOItem"  class="Inbound_NavBtn"></Button>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<f:SimpleForm editable="false" layout="ResponsiveGridLayout" labelSpanXL="5" labelSpanL="4" labelSpanM="4" labelSpanS="4"
				adjustLabelSpan="false" emptySpanXL="2" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
				singleContainerFullSize="false" class="Inbound_SimpleForm">
				<f:content>
					<Label text="{i18n>Label18}" design="Bold" class="sapUiSmallMarginTop Inbound_SimpleForm"></Label>
					<Input value="" submit="onGetPoItemF4" class="Inbound_InputBrd" width="10rem"/>
				</f:content>
			</f:SimpleForm>
			<VBox >
				<Table mode="MultiSelect" items="{POITEMS>/PoItemNav/results}" >
					<columns>
						<Column minScreenWidth="Phone" demandPopin="true" hAlign="Left" width="8rem">
							<Label text="{i18n>Label18}" design="Bold" class="Unload_InputLbl"/>
						</Column>
						<Column minScreenWidth="Phone" demandPopin="true" hAlign="Left" width="8rem">
							<Label text="{i18n>Text21}" design="Bold" class="Unload_InputLbl"/>
						</Column>
						<Column minScreenWidth="Phone" demandPopin="true" hAlign="Left" width="5rem">
							<Label text="{i18n>Quantity}" design="Bold" class="Unload_InputLbl"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<Text text="{POITEMS>Ebeln}"/>
								<Text text="{POITEMS>Matnr}"/>
								<Text text="{POITEMS>Menge}"/>
								<!--	<Input value="{ITEM>Config16}" editable="{ITEM>bEdit}" width="6rem"/>
							<Input value="{ITEM>Config17}" editable="{ITEM>bEdit}" width="6rem"/>
							<Input value="{ITEM>Config18}" editable="{ITEM>bEdit}" width="6rem"/>
							<Input placeholder="{i18n>Uom}" editable="{ITEM>bEdit}" width="6rem" value="{ITEM>Config19}" valueHelpOnly="true" showValueHelp="true"
								valueHelpRequest="onValueHelpPressUom"/>
							<Input value="{ITEM>Config20}" editable="false" width="8rem"/>
							<Button type="Reject" icon="sap-icon://delete" press="OnDeleteWBID" width="2.5rem"/>-->
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</VBox>
		</content>
		<endButton>
			<Button text="{i18n>OK}" press="OnAddPOItemWBID" class="Inbound_BtnClr"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>