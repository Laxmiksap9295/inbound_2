<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ZGT_MM_INBOUND.controller.Approve"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<!-- Vehicle Assignment Detail-1-->
	<Page id="id_VehicleAssign"  enableScrolling="true" >
		<customHeader>
			<Bar class="Inbound_Bar1">
				<contentLeft>
					<Button icon="sap-icon://nav-back" press="onNavBack" tooltip="{i18n>Tooltip2}" class="Inbound_NavBtn"></Button>
				</contentLeft>
				<contentMiddle>
					<Label text="{i18n>Title18}" class="Inbound_Title" design="Bold"/>
				</contentMiddle>
				   <contentRight>
                    <SearchField id="id_SearchField" liveChange="fnSearchRefId" class="sapUiSizeCompact" width="50%"/>
                    <Button icon="sap-icon://filter" press="_openPlantFragment" class="InboundBtnIcon"></Button>
                </contentRight>
			</Bar>
		</customHeader>
		<content>
			<ScrollContainer horizontal="true" visible="true" vertical="true" width="100%" height="100%">
				<Table id="vech_ass" class="newTableClass_CSQ" items="{oApproveModel>/ApprovalNav/results}" >
					<columns>
						<Column minScreenWidth="Phone" demandPopin="true" hAlign="Center" width="9rem">
							<Label text="{i18n>Label3}" design="Bold" class="tableheaderClass_CSQ"/>
						</Column>
						<Column minScreenWidth="Phone" demandPopin="true" hAlign="Center">
							<Label text="{i18n>Label6}" design="Bold" class="tableheaderClass_CSQ"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem type="Active" press="onPressListItem">
							<cells>
								<Text text="{oApproveModel>Wbid}"></Text>
								<!--<Text text="{path:'HEADER>Erdat',formatter:'ZGT_MM_INBOUND.Util.Formatter.Date'}"></Text>-->
								<Text text="{oApproveModel>Vehno}"/>
								
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</ScrollContainer>
		</content>
	</Page>
</mvc:View>